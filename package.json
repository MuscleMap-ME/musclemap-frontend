{
  "name": "musclemap",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "cleanup": "DRY_RUN=1 sh ./scripts/repo-cleanup.sh",
    "cleanup:apply": "APPLY=1 sh ./scripts/repo-cleanup.sh",
    "dev": "vite",
    "generate:icons": "node scripts/generate-icons.cjs",
    "prepare:appstore": "node scripts/prepare-app-store.cjs",
    "docs:generate": "node scripts/generate-docs.cjs",
    "docs:latex": "node scripts/generate-docs.cjs --latex",
    "docs:md": "node scripts/generate-docs.cjs --md",
    "docs:sync-plain": "node scripts/generate-docs.cjs --sync-plain",
    "build": "pnpm docs:sync-plain && vite build",
    "build:analyze": "ANALYZE=true vite build && echo 'Bundle analysis available at dist/stats.html'",
    "build:api": "pnpm -C apps/api build",
    "build:packages": "pnpm -C packages/shared build && pnpm -C packages/core build && pnpm -C packages/plugin-sdk build && pnpm -C packages/client build && pnpm -C packages/ui build",
    "build:all": "pnpm build:packages && pnpm build:api && pnpm build",
    "build:safe": "sh ./scripts/build-safe.sh",
    "build:smart": "node scripts/smart-build.mjs",
    "build:smart:force": "node scripts/smart-build.mjs --force",
    "build:smart:stats": "node scripts/smart-build.mjs --stats",
    "build:cache:clear": "node scripts/smart-build.mjs --clear",
    "build:cache:warm": "node scripts/cache-manager.mjs warm",
    "build:cache:analyze": "node scripts/cache-manager.mjs analyze",
    "build:cache:status": "node scripts/cache-manager.mjs stats",
    "build:compress": "sh ./scripts/compress-assets.sh",
    "build:turbo": "node scripts/smart-build.mjs --turbo",
    "build:vendors": "node scripts/prebundle-vendors.mjs",
    "build:vendors:force": "node scripts/prebundle-vendors.mjs --force",
    "build:vendors:status": "node scripts/prebundle-vendors.mjs --status",
    "transform-cache:save": "node scripts/transform-cache.mjs save",
    "transform-cache:restore": "node scripts/transform-cache.mjs restore",
    "transform-cache:status": "node scripts/transform-cache.mjs status",
    "transform-cache:clear": "node scripts/transform-cache.mjs clear",
    "cron:smoke": "node cron-jobs.js --smoke-test",
    "lint": "eslint src/ && pnpm -C apps/api lint && pnpm -C packages/core lint && pnpm -C packages/plugin-sdk lint",
    "lint:frontend": "eslint src/",
    "typecheck": "pnpm -C apps/api typecheck && pnpm -C packages/core typecheck && pnpm -C packages/shared typecheck && pnpm -C packages/plugin-sdk typecheck",
    "test": "pnpm test:api && pnpm test:frontend",
    "test:frontend": "vitest run",
    "test:frontend:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:api": "npx tsx scripts/e2e-user-journey.ts",
    "test:e2e:api:prod": "npx tsx scripts/e2e-user-journey.ts --production",
    "test:api": "pnpm -C apps/api test",
    "test:harness": "tsx scripts/test-harness/index.ts",
    "test:harness:core": "tsx scripts/test-harness/index.ts --category core",
    "test:harness:auth": "tsx scripts/test-harness/index.ts --category auth",
    "test:harness:prod": "tsx scripts/test-harness/index.ts --env production",
    "test:frontend-health": "npx tsx scripts/frontend-health-check.ts",
    "test:frontend-health:prod": "npx tsx scripts/frontend-health-check.ts --production",
    "test:frontend-health:local": "npx tsx scripts/frontend-health-check.ts --local",
    "test:coverage": "pnpm test:frontend:coverage && pnpm -C apps/api test:coverage",
    "deploy": "sh ./scripts/deploy-branch.sh",
    "deploy:now": "sh ./scripts/deploy-branch.sh --deploy --skip-tests",
    "deploy:auto": "sh ./scripts/deploy-branch.sh --auto-merge",
    "deploy:dry": "sh ./scripts/deploy-branch.sh --dry-run",
    "deploy:merge": "sh ./scripts/deploy-branch.sh --merge-now",
    "competitive-analysis": "sh ./scripts/competitive-analysis.sh",
    "competitive-analysis:check": "sh ./scripts/competitive-analysis.sh --check",
    "bug-hunter": "tsx scripts/bug-hunter/index.ts",
    "bug-hunter:daemon": "tsx scripts/bug-hunter/index.ts --daemon",
    "bug-hunter:aggressive": "tsx scripts/bug-hunter/index.ts --aggressive",
    "bug-hunter:production": "tsx scripts/bug-hunter/index.ts --production",
    "bug-hunter:status": "tsx scripts/bug-hunter/index.ts --status",
    "bug-hunter:coverage": "tsx scripts/bug-hunter/index.ts --coverage",
    "bug-hunter:queue": "tsx scripts/bug-hunter/index.ts --queue",
    "bug-hunter:process": "tsx scripts/bug-hunter/index.ts --process-queue",
    "validate:graphql": "npx tsx scripts/validate-graphql.ts",
    "validate:graphql:fix": "npx tsx scripts/validate-graphql.ts --fix",
    "validate:all": "pnpm typecheck && pnpm lint && pnpm validate:graphql"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@dicebear/collection": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@musclemap/client": "workspace:*",
    "@musclemap/shared": "workspace:*",
    "@phosphor-icons/react": "^2.1.10",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/d3": "^7.4.3",
    "apollo3-cache-persist": "^0.15.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.26",
    "graphql": "^16.12.0",
    "idb": "^8.0.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.562.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.2",
    "react-window": "^2.2.5",
    "reactflow": "^11.11.4",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "three": "^0.160.1",
    "uuid": "^13.0.0",
    "vanilla-tilt": "^1.8.1",
    "web-vitals": "^5.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.1.18",
    "vite": "^5.4.21",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.16"
  },
  "pnpm": {
    "overrides": {
      "@fastify/sensible": "^6.0.0",
      "@fastify/swagger": "^9.0.0",
      "@fastify/swagger-ui": "^5.0.0"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "sharp"
    ]
  }
}
