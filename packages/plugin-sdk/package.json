{
  "name": "@musclemap/plugin-sdk",
  "version": "2.0.0",
  "description": "SDK for building MuscleMap plugins",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./backend": "./dist/backend.js",
    "./frontend": "./dist/frontend.js"
  },
  "scripts": {
    "build": "bun run ../../scripts/bun-build-package.ts .",
    "build:tsc": "tsc",
    "dev": "bun run ../../scripts/bun-build-package.ts . --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@musclemap.me/core": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.18.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanpaulniko/musclemap.git",
    "directory": "packages/plugin-sdk"
  },
  "keywords": [
    "musclemap",
    "fitness",
    "plugin",
    "sdk"
  ],
  "author": "Jean Paul Niko",
  "license": "MIT"
}
