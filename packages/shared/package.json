{
  "name": "@musclemap.me/shared",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared utilities and constants for MuscleMap apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanpaulniko/musclemap.git",
    "directory": "packages/shared"
  },
  "keywords": [
    "musclemap",
    "fitness",
    "shared",
    "utilities"
  ],
  "author": "Jean Paul Niko",
  "license": "MIT"
}
