{
  "name": "@musclemap.me/client",
  "version": "1.0.0",
  "description": "Shared client-side business logic for MuscleMap",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./storage/web": {
      "types": "./dist/storage/web.d.ts",
      "import": "./dist/storage/web.mjs",
      "require": "./dist/storage/web.js"
    },
    "./storage/native": {
      "types": "./dist/storage/native.d.ts",
      "import": "./dist/storage/native.mjs",
      "require": "./dist/storage/native.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "expo-secure-store": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "expo-secure-store": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "expo-secure-store": "~14.0.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanpaulniko/musclemap.git",
    "directory": "packages/client"
  },
  "keywords": [
    "musclemap",
    "fitness",
    "api-client",
    "sdk"
  ],
  "author": "Jean Paul Niko",
  "license": "MIT"
}
