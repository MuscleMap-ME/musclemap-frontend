{
  "name": "@musclemap.me/ui",
  "version": "1.0.0",
  "description": "Shared cross-platform UI components for MuscleMap",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tamagui.config": {
      "types": "./dist/tamagui.config.d.ts",
      "import": "./dist/tamagui.config.mjs",
      "require": "./dist/tamagui.config.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-native": ">=0.74.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "@tamagui/animations-react-native": "^1.120.0",
    "@tamagui/config": "^1.120.0",
    "@tamagui/core": "^1.120.0",
    "@tamagui/lucide-icons": "^1.120.0",
    "tamagui": "^1.120.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanpaulniko/musclemap.git",
    "directory": "packages/ui"
  },
  "keywords": [
    "musclemap",
    "fitness",
    "ui",
    "components",
    "tamagui"
  ],
  "author": "Jean Paul Niko",
  "license": "MIT"
}
