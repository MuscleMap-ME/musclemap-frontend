{
  "name": "@musclemap/contracts",
  "version": "1.0.0",
  "description": "Shared GraphQL schema and types for MuscleMap",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema/index.js",
      "types": "./dist/schema/index.d.ts"
    },
    "./schema.graphql": "./dist/schema.graphql"
  },
  "files": [
    "dist",
    "src/schema/**/*.graphql"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/schema/index.ts --format esm --dts --clean",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanpaulniko/musclemap.git",
    "directory": "packages/contracts"
  },
  "keywords": [
    "musclemap",
    "graphql",
    "schema",
    "types"
  ],
  "author": "Jean Paul Niko",
  "license": "MIT"
}
