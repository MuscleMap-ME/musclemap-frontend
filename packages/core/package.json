{
  "name": "@musclemap/core",
  "version": "2.0.0",
  "description": "Shared domain types and utilities for MuscleMap",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun ../../scripts/bun-build-package.ts .",
    "build:tsc": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
