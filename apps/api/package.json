{
  "name": "@musclemap/api",
  "version": "2.0.0",
  "description": "MuscleMap API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:integrity": "vitest run src/tests/data-integrity.test.ts",
    "test:coverage": "vitest --coverage",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "test:watch": "vitest",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/fastify": "^3.1.0",
    "@fastify/compress": "^8.3.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.4",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/websocket": "^11.2.0",
    "@graphql-tools/schema": "^10.0.31",
    "@musclemap/core": "workspace:*",
    "@musclemap/native": "workspace:*",
    "@musclemap/plugin-sdk": "workspace:*",
    "@tensorflow/tfjs-node": "^4.22.0",
    "bullmq": "^5.66.4",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.0",
    "fastify": "^5.6.2",
    "graphql": "^16.12.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-scalars": "^1.25.0",
    "graphql-ws": "^6.0.6",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.4",
    "nsfwjs": "^4.2.1",
    "pg": "^8.13.0",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "sharp": "^0.34.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.18.0",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  }
}
