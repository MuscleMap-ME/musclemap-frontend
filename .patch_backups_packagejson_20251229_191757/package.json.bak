{
  "name": "musclemap",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "server:watch": "nodemon server/index.js",
    "client": "vite",
    "build": "vite build",
    "build:check": "vite build && ls -lh dist/",
    "preview": "vite preview",
    "setup": "node scripts/setup-db.js",
    "setup:force": "rm -f musclemap.db && node scripts/setup-db.js",
    "start": "NODE_ENV=production node server/index.js",
    "test": "echo 'No tests yet' && exit 0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.13",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.26",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.10.3",
    "stripe": "^14.10.0",
    "three": "^0.160.0",
    "uuid": "^13.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^5.0.8"
  }
  {
  "scripts": {
    "cleanup": "DRY_RUN=1 sh ./scripts/repo-cleanup.sh",
    "cleanup:apply": "APPLY=1 sh ./scripts/repo-cleanup.sh"
  }
  }
}
